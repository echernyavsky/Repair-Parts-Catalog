@{
    ViewBag.Title = "Upload vehicle list";
}

<form action="@Url.Action("UploadVehicles", "Upload")" method="POST" enctype="multipart/form-data">
    <div class="form-group">
        <input type="file" id="file" name="file" />
    </div>
    <div class="form-group">
        <input type="submit" value="Upload" class="btn btn-primary" />
    </div>

</form>
